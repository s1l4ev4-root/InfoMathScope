{
  "files": [],
  "compilerOptions": {
    "paths": {
      "@public/*": [
        "./public/*"
      ],
      "@fonts/*": [
        "./public/fonts/*"
      ],
      "@icons/*": [
        "./public/icons/*"
      ],
      "@images/*": [
        "./public/images/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@features/*": [
        "./src/features/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ],
    },
  },
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ]
}